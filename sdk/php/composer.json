{
  "name": "kucoin/kucoin-universal-sdk",
  "type": "library",
  "license": "MIT",
  "description": "Official KuCoin Universal SDK",
  "keywords": [
    "kucoin",
    "api",
    "sdk",
    "cryptocurrency",
    "trading",
    "exchange"
  ],
  "homepage": "https://github.com/kucoin/kucoin-universal-sdk",
  "support": {
    "source": "https://github.com/kucoin/kucoin-universal-sdk",
    "issues": "https://github.com/kucoin/kucoin-universal-sdk/issues"
  },
  "authors": [
    {
      "name": "KuCoin",
      "email": "api@kucoin.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "jms/serializer": "^3.32",
    "doctrine/annotations": "^2.0",
    "guzzlehttp/guzzle": "^7.9",
    "ratchet/pawl": "^0.4.3",
    "react/event-loop": "^1.5",
    "react/promise": "^3.2",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "ramsey/uuid": "^4.7",
    "react/async": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "KuCoin\\UniversalSDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "test/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "secure-http": true
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
